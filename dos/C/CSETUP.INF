;
; INFORMATION FILE FOR C 7.0 SETUP
;

[macro]
VERS = "7.00"

; default directory names (must end with \)
DEFDIRBASE     = "C:\C700\"
DEFDIRBIN      = "BIN\"
DEFDIRHELP     = "HELP\"
DEFDIRINCL     = "INCLUDE\"
DEFDIRINIT     = "INIT\"
DEFDIRLIB      = "LIB\"
DEFDIRMFC      = "MFC\"
DEFDIRSOURCE   = "SOURCE\"
DEFDIRSAMPLES  = "SAMPLES\"
DEFDIRTMP      = ""

PACKINGLIST = "PACKING.TXT"
READMEFILE  = "README.TXT"

; This definition is needed ...
SETUP_VER   =
MS_SETUP    = "Microsoft (R) Setup Version"
COPYRIGHT   = "Copyright (C) Microsoft Corp. 1986, 1992. All rights reserved."

; filename of linker, and backup name used during version checking
LINK_NAME      = "link.exe"
TEMP_LINK_NAME = "$$$link.tmp"


; ************** lists ********************

;;; Insert Source Media Description here ... VVVVVV ....

[list sdisks]
0:dos, "Your MS-DOS SYSTEM disk"
1:x,   "Disk1: Setup"
2:x,   "Disk2: Utilities"
3:x,   "Disk3: C and C++ Compiler"
4:x,   "Disk4: Help Files - I"
5:x,   "Disk5: Library Files"
6:x,   "Disk6: Codeview"
7:x,   "Disk7: Microsoft Foundation Classes - I"
8:x,   "Disk8: Microsoft Foundation Classes - II"
9:x,   "Disk9: Microsoft Foundation Classes - III"

; not only must these paths be relative to setup.exe, they must be
; relative to each other, so that from within any dir, the relative
; path will get you to any other dir.  This means that setup.exe must
; be in one of the named dirs.
[list netsetup]
0:x,  " "
1:x,  "..\disk1"
2:x,  "..\disk2"
3:x,  "..\disk3"
4:x,  "..\disk4"
5:x,  "..\disk5"
6:x,  "..\disk6"
7:x,  "..\disk7"
8:x,  "..\disk8"
9:x,  "..\disk9"

;;; End of Source Media Description here ... ^^^^^^ ....

; very rarely used
[list ddisks]
1:x,  "the destination disk, tape, or cartridge"


; LIBRARY-BUILDING LISTS and MACROS

[macro]
LIB_EXE       = "LIB.EXE"

; These are used in the building of the mondo library names
LIB_BASE      = "LIBC"
LIBDLL_BASE   = "DLLC"
MATH_LETTERS  = "E7A"
MODEL_LETTERS = "SMCL"
MODE_LETTERS  = "RWQW"

